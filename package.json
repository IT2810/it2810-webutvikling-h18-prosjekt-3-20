{
  "name": "my-new-project",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "lint": "eslint . -f stylish",
    "test": "node ./node_modules/jest-expo/bin/jest.js",
    "test:watch": "node ./node_modules/jest-expo/bin/jest.js --watchAll",
    "precommit": "npm run lint",
    "prepush": "node ./node_modules/jest-expo/bin/jest.js --coverage"
  },
  "jest": {
    "preset": "jest-expo",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "expo": "^30.0.1",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-calendars": "^1.21.0",
    "react-navigation": "^2.16.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.3.0",
    "husky": "^1.1.0",
    "jest-expo": "30.0.0"
  }
}
